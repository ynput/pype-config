{
  "IntegrateMasterVersion": {
    "enabled": false
  },
  "ExtractReview": {
    "__documentation__": "http://pype.club/docs/admin_presets_plugins",
    "ext_filter": ["exr", "jpg", "jpeg", "mov", "png", "dpx", "mp4"],
    "outputs": {
      "h264": {
        "families": ["render", "ftrack"],
        "input": [
          "-gamma 2.2"
        ],
        "codec": [
          "-pix_fmt yuv420p",
          "-crf 18",
          "-intra"
        ],
        "tags": ["burnin", "preview"],
        "ext": "mov"
      }
    }
  }
}
